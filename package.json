{
  "name": "conditionalize",
  "version": "0.0.5-alpha",
  "author": "Luis",
  "license": "MIT",
  "main": "lib/conditionalize.js",
  "module": "es/conditionalize.js",
  "browser": "dist/conditionalize.min.js",
  "description": "A library of conditions builder and validator",
  "repository": {
    "type": "git",
    "url": "https://github.com/laurey/conditionalize.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "build:browser": "rollup -c --environment BABEL_ENV:browser,NODE_ENV:production --env=browser",
    "build:es": "rollup -c --environment BABEL_ENV:es,NODE_ENV:production --env=es",
    "build:lib": "rollup -c --environment BABEL_ENV:cjs,NODE_ENV:production --env=cjs",
    "build": "run-p build:*",
    "clean": "run-p clean:*",
    "clean:es": "rimraf es",
    "clean:node": "rimraf lib",
    "clean:browser": "rimraf dist",
    "coverage": "jest --coverage",
    "dev": "run-p serve watch",
    "start": "nodemon --exec 'npm run dev'",
    "prebuild": "npm run clean",
    "release": "np",
    "serve": "serve public",
    "test": "jest",
    "test:watch": "jest --watch",
    "version": "npm run build",
    "watch": "rollup -c -w"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.17.7",
    "@babel/eslint-plugin": "^7.17.7",
    "@babel/node": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-proposal-decorators": "^7.16.0",
    "@babel/plugin-proposal-export-default-from": "^7.16.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.16.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.16.0",
    "@babel/plugin-proposal-numeric-separator": "^7.16.0",
    "@babel/plugin-proposal-optional-chaining": "^7.16.0",
    "@babel/plugin-proposal-private-methods": "^7.16.0",
    "@babel/plugin-proposal-private-property-in-object": "^7.16.0",
    "@babel/plugin-transform-modules-commonjs": "^7.16.0",
    "@babel/plugin-transform-modules-umd": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/runtime": "^7.16.0",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "babel-plugin-add-module-exports": "^1.0.4",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.15.0",
    "jest": "^29.3.0",
    "lodash": "^4.17.20",
    "np": "^7.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.22.2",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-dts": "^1.4.6",
    "rollup-plugin-external-globals": "^0.6.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-terser": "^7.0.0",
    "typescript": "^4.1.2"
  },
  "peerDependencies": {
    "lodash": "*"
  },
  "engines": {
    "node": ">=14.x"
  },
  "files": [
    "es",
    "esm",
    "lib",
    "dist"
  ],
  "keywords": [
    "assert",
    "condition",
    "conditional",
    "when",
    "where",
    "validate",
    "validator",
    "validation",
    "promise",
    "es6",
    "commonjs",
    "browser",
    "node"
  ]
}
